{
    "name": "ucraft-com/ql-parser",
    "version": "1.0.0",
    "description": "Converts Ucraft migrator Ql",
    "authors": [
        {
            "name": "Miqayel Srapionyan",
            "email": "miqayel@ucraft.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "QLParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QLParser\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QLParser\\Providers\\QLParserServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^12.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev"
}
